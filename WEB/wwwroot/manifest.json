{
	"_version": "1.21.0",
	"sap.app": {
		"id": "PedroAutoPecas",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"title": "appTitle",
		"description": "appDescription",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "PedroAutoPecas.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "PedroAutoPecas.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "PedroAutoPecas.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
            	"async": true				
			},
			"routes": [
				{
					"pattern": "",
					"name": "Peca",
					"target": "listaDePecas"
				},
				{
					"pattern": "Detalhe/{id}",
					"name": "Detalhe",
					"target": "Detalhe"
				}
			],
			"targets": {
				"listaDePecas": {
					"id": "listaDePecasId",
					"name": "ListaDePecas"
				},
				"Detalhe": {
					"id": "DetalheId",
					"name": "Detalhe"
				}
			}
		}
	}
}
